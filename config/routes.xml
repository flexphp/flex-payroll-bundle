<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <!--
    Add routes in config/routes.yaml
    flexphp_payroll_bundle_routes:
        resource: "@FlexPHPPayrollBundle/config/routes.xml"
        prefix: /
    -->

    <route id="flexphp.payroll.employee-types.index" controller="flexphp.payroll.bundle.employee_type_controller::index" path="/employee-types" methods="GET" />
    <route id="flexphp.payroll.employee-types.new" controller="flexphp.payroll.bundle.employee_type_controller::new" path="/employee-types/new" methods="GET" />
    <route id="flexphp.payroll.employee-types.create" controller="flexphp.payroll.bundle.employee_type_controller::create" path="/employee-types/create" methods="POST" />
    <route id="flexphp.payroll.employee-types.read" controller="flexphp.payroll.bundle.employee_type_controller::read" path="/employee-types/{id}" methods="GET" />
    <route id="flexphp.payroll.employee-types.edit" controller="flexphp.payroll.bundle.employee_type_controller::edit" path="/employee-types/edit/{id}" methods="GET" />
    <route id="flexphp.payroll.employee-types.update" controller="flexphp.payroll.bundle.employee_type_controller::update" path="/employee-types/update/{id}" methods="PUT" />
    <route id="flexphp.payroll.employee-types.delete" controller="flexphp.payroll.bundle.employee_type_controller::delete" path="/employee-types/delete/{id}" methods="DELETE" />

